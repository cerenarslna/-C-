using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace boolveri;

internal class boolveri
{
    static void Main(string[] args)
    {
       //Bool : mantıksal bir veri türüdür. True veya false saklar.
       bool durum = true;
       durum = 10 > 20;
       Console.WriteLine(durum);
       Console.ReadLine();
    }
}

